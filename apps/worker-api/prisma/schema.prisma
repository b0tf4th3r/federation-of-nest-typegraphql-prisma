datasource db {
  provider = "postgresql"
  url      = env("WORKER_API_DATABASE_URL")
}

// ! generators

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x"]
}

generator typegraphql {
  provider = "typegraphql-prisma"
  output   = "../__generated__/typegraphql-prisma"
}

